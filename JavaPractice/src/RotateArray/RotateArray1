package RotateArray;

public class RotateArray1 {
	
	public static void main(String args[]) {
		
		
		int a[] = {1,2,3,4,5,6,7};
		
		int temp = 0;
		
		for(int i=0; i<4; i++) {
			
			temp = a[a.length-1];
			
			for(int j=0; j<a.length-1; j++) {
			
			a[(a.length-j)-1] = a[(a.length-j)-2];
	
			}
			a[0] = temp;
		}
		
		for(int aa: a) {
			System.out.print(aa+ ",");
		}
		
	}

}
